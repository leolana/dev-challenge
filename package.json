{
  "name": "marketing-twitter-integration",
  "version": "1.0.0",
  "description": "Ferramenta para acompanhar o engajamento da marca na rede social \"Twitter\"",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean:dist",
    "build": "cross-env NODE_ENV=production babel -s true ./server.js -d dist",
    "start": "npm run serve:dev ./server.js",
    "serve:dev": "nodemon $2 --exec babel-node",
    "serve:dist": "nodemon dist/index.js",
    "test": "cross-env NODE_ENV=test nyc mocha --require babel-polyfill --compilers js:babel-register --recursive tests/",
    "clean:dist": "shx rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leolana/dev-challenge.git"
  },
  "author": "leolana",
  "license": "MIT",
