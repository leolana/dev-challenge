    "url": "https://github.com/leolana/dev-challenge/issues"
  },
  "homepage": "https://github.com/leolana/dev-challenge#readme",
  "dependencies": {
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-compress": "^2.0.0",
    "koa-cors": "0.0.16",
    "koa-cors-error": "0.0.2",
    "koa-mount": "^3.0.0",
    "koa-react-view": "^3.0.0",
    "koa-router": "^7.2.1",
    "koa-static-cache": "^5.1.1",
    "material-ui": "^0.19.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "twitter": "^1.7.1",
    "unescape-html": "^1.1.0"
  },
